{
  "name": "ngx-center-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn build:center-dll:prod && yarn build:center-main:prod && yarn build:project1:prod && npx cpx \"./dist/project1/**/*\" ./dist/ngx-center-template/project1",
    "serve:build": "http-server ./dist/ngx-center-template",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:center-main": "ng serve --configuration center-main",
    "build:center-main": "ng build --configuration center-main",
    "build:center-main:prod": "ng build --configuration center-main-prod",
    "build:center-dll": "ng build --configuration center-dll",
    "build:center-dll:prod": "ng build --configuration center-dll-prod",
    "build:project1": "ng build project1",
    "build:project1:prod": "ng build project1 --prod",
    "start:project1": "ng serve project1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "ngx-center": "^0.2.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.0.0",
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "cpx": "^1.5.0",
    "http-server": "^13.0.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5",
    "webpack-bootstrap-assets-plugin": "2.0.0",
    "webpack-ng-dll-plugin": "2.2.2"
  }
}
