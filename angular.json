{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-center-template": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/ngx-center-template",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "center-dll": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "outputPath": "dist/center-dll",
              "assets": [],
              "styles": [],
              "scripts": [],
              "customWebpackConfig": {
                "path": "./webpack.config.center-dll.ts"
              },
              "tsConfig": "tsconfig.dll.json"
            },
            "center-main": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "index": {
                "input": "src/index.center-dll.html",
                "output": "index.html"
              },
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "dll.js",
                  "input": "./dist/center-dll",
                  "output": "./"
                }
              ],
              "customWebpackConfig": {
                "path": "./webpack.config.center-main.ts"
              }
            },
            "center-main-prod": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extractLicenses": false,
              "vendorChunk": false,
              "index": {
                "input": "src/index.center-dll.html",
                "output": "index.html"
              },
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "dll.js",
                  "input": "./dist/center-dll",
                  "output": "./"
                }
              ],
              "customWebpackConfig": {
                "path": "./webpack.config.center-main.ts"
              }
            },
            "center-dll-prod": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extractLicenses": false,
              "outputPath": "dist/center-dll",
              "vendorChunk": false,
              "assets": [],
              "styles": [],
              "scripts": [],
              "customWebpackConfig": {
                "path": "./webpack.config.center-dll.ts"
              },
              "tsConfig": "tsconfig.dll.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ngx-center-template:build:production"
            },
            "development": {
              "browserTarget": "ngx-center-template:build:development"
            },
            "center-main": {
              "browserTarget": "ngx-center-template:build:center-main"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-center-template:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        }
      }
    },
    "project1": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/project1",
      "sourceRoot": "projects/project1/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/project1",
            "index": "",
            "main": "projects/project1/src/main.ts",
            "polyfills": "",
            "tsConfig": "projects/project1/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/project1/src/favicon.ico",
              "projects/project1/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "customWebpackConfig": {
              "path": "./webpack.config.project1.ts"
            },
            "vendorChunk": false,
            "deployUrl": "http://127.0.0.1:4222/"
          },
          "configurations": {
            "production": {
              "extractLicenses": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/project1/src/environments/environment.ts",
                  "with": "projects/project1/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "deployUrl": "project1/"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "project1:build:production"
            },
            "development": {
              "browserTarget": "project1:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "publicHost": "0.0.0.0:4222",
            "port": 4222,
            "servePath": "/",
            "disableHostCheck": true
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "project1:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/project1/src/test.ts",
            "polyfills": "projects/project1/src/polyfills.ts",
            "tsConfig": "projects/project1/tsconfig.spec.json",
            "karmaConfig": "projects/project1/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/project1/src/favicon.ico",
              "projects/project1/src/assets"
            ],
            "styles": ["projects/project1/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "ngx-center-template"
}
